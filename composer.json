{
    "name": "fengxin8144/laravel_cas_server",
    "description": "A laravel package provides CAS server implementation , copy to leo108/laravel_cas_server",
    "type": "library",
    "license": "Apache-2.0",

    "require": {
        "php": ">=7.0",
        "ext-dom": "*",
        "illuminate/support": "6.*",
        "doctrine/dbal": "^2.5",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.0|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "autoload": {
        "psr-4": {
            "fengxin8144\\CAS\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/_support/SerializableModel.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "fengxin8144\\CAS\\CASServerServiceProvider"
            ]
        }
    }
}
